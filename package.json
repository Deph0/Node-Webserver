{
  "name": "node-webserver",
  "version": "1.0.2",
  "description": "An easy-to-use webserver powered by Node and ExpressJS",
  "main": "dist/src/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build && node dist/src/app.js",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 ./src/**/*.html ./src/**/*.css ./src/**/*.png ./src/**/*.ico ./src/**/*.jpg ./src/**/*.jpeg ./src/**/*.gif ./src/**/*.mp3 ./src/**/*.mp4 ./dist/src/",
    "build": "npm run clean && tsc && npm run copy-files && npm run minify",
    "minify": "npm run uglify",
    "uglify": "uglifyjs ./dist/src/app.js -o ./dist/src/app.js"
  },
  "keywords": [],
  "author": "Logan Brown",
  "license": "ISC",
  "dependencies": {
    "cluster": "^0.7.7",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-asyncify": "^1.1.0",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "hpp": "^0.2.3",
    "html-minifier": "^4.0.0",
    "morgan": "^1.10.0",
    "path": "^0.12.7",
    "typescript": "^4.9.5",
    "uglify-js": "^3.17.4",
    "vhost": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^18.14.1",
    "copyfiles": "^2.4.1",
    "rimraf": "^4.1.2"
  }
}
